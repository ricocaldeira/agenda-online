<%= simple_form_for(@contact_field) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :field_type %>
    <%= f.input :required %>
    <%= f.association :contact_schema %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
